#include <bits/stdc++.h>

using namespace std;
//exercice 11875

int main(){

    int N,T,i,c=0,age[20];

    scanf("%d",&T);
    while(T--){
        scanf("%d",&N);
        for (i = 0; i < N; i++){
            scanf("%d",&age[i]);
        }
        sort(age,age+N);
        i=N/2;
        printf("Case %d: %d\n",++c,age[i]);
    }

    return 0;
}
