//http://codeforces.com/problemset/problem/236/A
#include <bits/stdc++.h>

using namespace std;

bool is_odd_distinct_char(string s){
  
  set < char > letters;

  for(int i=0; i<(int)s.size(); i++)
    letters.insert(s[i]);

  int len = (int)letters.size();

  if(len % 2 != 0)
    return true;
  else 
    return false;
}

int main() {
 

  string name;
  cin >> name;

  if(is_odd_distinct_char(name))
    cout << "IGNORE HIM!" << endl;
  else cout << "CHAT WITH HER!" << endl;

  return 0;
}
